# Example of complex filter with AND/OR conditions (disabled by default)
name: complex-filter
type: expression
enabled: true
description: "Block simulated SSH alerts from internal IPs"
filter:
  op: and
  conditions:
    - field: simulated
      op: eq
      value: true
    - op: or
      conditions:
        - field: scenario
          op: glob
          value: "*ssh*"
        - field: source.ip
          op: cidr
          value:
            - "192.168.0.0/16"
